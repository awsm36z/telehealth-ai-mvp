{
  "cli": {
    "version": ">= 18.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "dev-device": {
      "extends": "development"
    },
    "dev-sim": {
      "extends": "dev-device",
      "ios": {
        "simulator": true
      }
    },
    "dev-sim-tablet": {
      "extends": "dev-sim",
      "env": {
        "IOS_SIMULATOR_DEVICE": "iPad Pro 13-inch (M5)"
      }
    },
    "dev-sim-ipad-mini": {
      "extends": "dev-sim",
      "env": {
        "IOS_SIMULATOR_DEVICE": "iPad mini (A17 Pro)"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "preview-device": {
      "extends": "preview"
    },
    "preview-sim": {
      "extends": "preview-device",
      "ios": { "simulator": true }
    }
  },
  "submit": {
    "production": {}
  }
}
